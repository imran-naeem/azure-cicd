{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf24oct"
		},
		"AKVLS_cicd_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://anwarakv.vault.azure.net/"
		},
		"gitcicd_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "akv-blob-connection"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf24oct_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AKVLS_cicd_properties_typeProperties_baseUrl": {
						"value": "[parameters('AKVLS_cicd_properties_typeProperties_baseUrl')]"
					},
					"gitcicd_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('gitcicd_properties_typeProperties_connectionString_secretName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}